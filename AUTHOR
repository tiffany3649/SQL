SELECT * FROM COURSES LIMIT 3;
SELECT * FROM TEXTBOOKS LIMIT 3;
SELECT * FROM AUTHOR LIMIT 3;
SELECT * FROM AUTHORTEXTBOOKLINK LIMIT 3;

SELECT COURSENAME, SUBJECT, CURRENT
FROM COURSES
WHERE SUBJECT= 'Economics'
AND CURRENT = 'Yes';

SELECT COURSENAME, SUBJECT, TEXTBOOKS.TITLE
FROM COURSES
INNER JOIN TEXTBOOKS
ON TEXTBOOKS.COURSEID = COURSES.COURSEID
WHERE SUBJECT= 'Economics'
AND CURRENT = 'Yes'

LEFT JOIN AUTHORTEXTBOOKLINK
ON AUTHOR
